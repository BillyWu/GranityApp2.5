	function f_mdown()
	{
			currentMoveObj = document.getElementById("PopupDiv");                //当对象被按下时，记录该对象
			if(event.x > currentMoveObj.style.pixelLeft)
				relLeft = event.x - currentMoveObj.style.pixelLeft;
			if(event.y > currentMoveObj.style.pixelTop)
				relTop = event.y - currentMoveObj.style.pixelTop;
			zindex=currentMoveObj.style.zIndex;//记录原z-index值
			currentMoveObj.style.zIndex=10000;
	}
	window.document.onmouseup = function()
	{      
		if(!currentMoveObj) return;
		currentMoveObj.style.zIndex=zindex;//恢复
	     zindex=-1;
		currentMoveObj = null;        //当鼠标释放时同时释放拖动对象
     }

	function f_move(obj)
	{
        if(currentMoveObj != null)
        {
        	var frobj=document.getElementById("DivShim");
        	if(event.x-relLeft>0)
        	{
            	currentMoveObj.style.pixelLeft=event.x-relLeft;
            	frobj.style.pixelLeft=event.x-relLeft;
            }
            if(event.y-relTop>0)
            {
            	currentMoveObj.style.pixelTop=event.y-relTop;
				frobj.style.pixelTop=event.y-relTop;
            }
        }
	}
   
	function DivSetVisible(state)
	{
		var DivRef = document.getElementById('PopupDiv');
		var IfrRef = document.getElementById('DivShim');
		if(state)
		{
			DivRef.style.display = "block";
			IfrRef.style.width = DivRef.offsetWidth;
			IfrRef.style.height = DivRef.offsetHeight;
			IfrRef.style.top = DivRef.style.top;
			IfrRef.style.left = DivRef.style.left;
			IfrRef.style.zIndex = DivRef.style.zIndex - 1;
			IfrRef.style.display = "block";
		}
		else
		{
			DivRef.style.display = "none";
			IfrRef.style.display = "none";
		}
	}
	
