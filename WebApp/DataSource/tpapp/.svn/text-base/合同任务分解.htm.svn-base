	function autoph()
	{
	 	var result = confirm("累加配货选【确定】，重新配货选【取消】？");
		if(!result) return;
		oband=document.UnitItem.getBandByItemName("goods");
	    for(var i=0;i<oband.RecordCount();i++)
	    {
	        var needx = parseFloat(oband.getFldStrValue("数量",i));
	        var storex = parseFloat(oband.getFldStrValue("库存",i));
			if(storex>=needx) 
			    oband.setFldStrValue(i,"配货量",needx);
			else 
				oband.setFldStrValue(i,"配货量",storex);
	    }
	}

