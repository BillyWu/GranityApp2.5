		function _import()
		{
			document.UnitItem.getBandByItemName("bom").isimport=false;
			var oband=document.UnitItem.getBandByItemName("slgoods");
			if(oband.RecordCount()==0) {alert("请选择要加入的货品！");document.getElementById("infindtext").onclick();document.getElementById("infindtext").focus(); return;}
			//如果是备选项目
			if(oband.exportItem && ""!=oband.exportItem)
			{
			    oband.ExportRow(oband.exportItem);
			    for(var i=0;i<document.UnitItem.Bands.length;i++)
			    {
			        //找到主表为band.itemname的所有子表
			        var osband = document.UnitItem.Bands[i];
			        if(osband.MasterItemName==oband.ItemName)
			        {
			            if(osband.exportItem && osband.exportItem!="")
			            osband.ExportRow(osband.exportItem);
			        }
			        //查找副表，将副表数据增加到目标数据源中
			    }
			}		
		}
		
		function NodeAdd(itemId,addnext,proot)
		{
			var mband=document.UnitItem.getBandByItemName("goods");
			if(mband.RecordCount()==0)
			{
				alert("请先增添货品后再定义物料结构！");
				return;
			}
			oband=document.UnitItem.getBandByItemName("合同bom");
			if(oband.RecordCount()==0 && proot!=true)
			{
			 	var result = confirm("请首先定义货品物料结构名称！确认吗？");
				if(!result) return;				
				TreeUtil.addNode(itemId,false,true);
			}
			else
				TreeUtil.addNode(itemId,addnext,proot);
				
			ms_showBoard('dstbom',1);			
			var oband=document.UnitItem.getBandByItemName("合同bom");
			node=oband.Tree.WebTree.SelectedNode;
							
			if(node.ParentNode)
			{
				oband.setFldStrValue(null,"编码",node.ParentNode.ID + "-"+(node.GetCurrentIndex()+1));				
				oband.setFldStrValue(null,"名称","物品"+(node.GetCurrentIndex()+1));
				ms_showBoard("dstbom",0);
			}
			else 
			{
				oband.setFldStrValue(null,"属性","结构"); 
				oband.setFldStrValue(null,"名称","结构"+(node.GetCurrentIndex()+1));	
				oband.setFldStrValue(null,"编码",mband.getFldStrValue(mband.linkColM) + "-"+(node.GetCurrentIndex()+1));				
				ms_showBoard("dstbom",1);
			}
		}
		
		function isExport()
		{
			oband=document.UnitItem.getBandByItemName("bom");
		    var chkId="";
		    if(oband.isimport) return;
		    for(var i=0;i<oband.RecordCount();i++)
		    {
		        if(oband.getFldStrValue("选择",i)=="-1")
		            chkId = oband.getFldStrValue(oband.Tree.IDField,i);
		        
		    }
			
			if(oband.Tree.WebTree.Nodes().length>1 && chkId=="") 
			{
		        {alert("该货品有多种物料结构，请选择一个！");oband.isimport=true;return false;}
			}
			return true;
		}
		
