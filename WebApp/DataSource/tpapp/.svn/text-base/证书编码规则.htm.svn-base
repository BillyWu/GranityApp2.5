	<table align="center" border="2" width="799" bordercolorlight="#000000" bordercolordark="#000000" style="border-collapse: collapse" id="table10" height="162">
			<tbody>
			<tr>
				<td colspan="6" align="center" style="font-family: 宋体; font-size: 10pt; font-weight: bold" bgcolor="#FBFAFA" height="24">
				编码规则设置</td>
			</tr>
			<tr>
				<td width="11%" align="right" height="28">证书：</td>
				<td width="31%" height="28">
		                 		<SELECT class="xlandselect" dataFld="证书" dataSrc="#MasterTab" size="1" style="WIDTH: 98%" name="" DataValueField="名称" DataTextField="名称" DataSource="dict_certificate">
					</SELECT></td>
				<td width="12%" height="28" align="right">审批事项：</td>
