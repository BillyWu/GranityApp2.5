                                <td>
                                    <input type="button" value="查询" name="B4" onclick="ue_Time();ue_tfilter('区域员工出场')"
                                        style="width: 68px" />
                                    <input type="button" value="打印" name="B3" class="standbtn" onclick="tp_print();"
