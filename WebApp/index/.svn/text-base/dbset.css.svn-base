.page{position:relative;height:100%;text-align:center;/*min-height:775px;min-width:930px;*/}/* reset */
html,body
{
height:100%;
margin:0px auto;
text-align:center;
}

body,input,select,button{font-family:Î¢ÈíÑÅºÚ,verdana,simsun,sans-serif;}
body,select,input,textarea,button,h1,h2,h3,h4,h5,h6,table{font-size:12px;}
body,h1,h2,h3,h4,h5,h6,ul,ol,li,form,p,dl,dt,dd,table,th,td,img,blockquote{margin:0;padding:0;border:0;}
input,button,textarea,img{line-height:normal;}
abbr,acronym,address,cite,q,em,code,var,dfn,ins{font-style:normal;text-decoration:none;border:0;}
q:before,q:after{content:"";}
ul,ol{list-style:none;}
table{border-collapse:collapse;border-spacing:0;}
select,input,button,button img,.btn{vertical-align:middle;}
.ipt-c,.ipt-r{width:16px;height:16px;padding:0;margin:0 3px 0 0;overflow:hidden;}
.no-ime{ime-mode:disabled;}
.ipt-b,
.ipt-b50,
.btn{font-weight:bold;font-size:14px;cursor:pointer}
.ipt-b{border:0;background:0;padding:0;}
.ipt-b50{border:0;background:0;padding:0;}
.btn{text-align:center;}
a{color:#0E7347;}


/* background */
.login .domain,
.zxdt,
.zxdt h3,
.zxdt .end,
.btn,
.ipt-b,
.ipt-b50,
.ipt-t,
.login h3,
.intro li,
.logo h2,
.bg{background-image:url(bgimg.jpg);background-repeat:no-repeat;}

	.login{width:390px;padding-left:0px;margin-top:56px;}
	.loginA{width:390px;padding-left:0px;margin-top:20px;}
	.login h3{background-position:0 -36px;width:192px;height:30px;margin:0 0 20px 3px;text-indent:-999em;overflow:hidden;}
	.login .fi{zoom:1;overflow:hidden;padding:5px 0;}
	.login .fi0{zoom:1;overflow:hidden;padding:0px 0;}
	.login .fi-btn{padding:10px 0;}
	.login .fi-nolb,
	.login .fi-btn{padding-left:51px;}
	.login .domain{font-size:14px;color:#8E8E8E;margin-left:10px;width:80px;height:13px;background-position:-216px -36px;display:-moz-inline-box;-moz-box-align:center;display:inline-block;vertical-align:middle;text-indent:-999em;overflow:hidden;}
	.login .lb{font-size:14px;color:#525252;float:left;width:42px;line-height:30px;margin-right:10px;_margin-right:7px;}
	.login .lb100{text-align:right;font-size:14px;color:#525252;float:left;width:100px;line-height:30px;margin-right:-80px;}
	.login .ipt-t{font-size:16px;color:#454545;font-weight:bold;border:1px solid #737373;padding:3px 2px 2px;width:203px;height:22px;background-position:0 -72px;margin-right:-5px;}
	.login .ipt-t-focus{background:#FEFBBD;}
	.login .btn,
	.login .ipt-b{width:90px;height:31px;color:#373737;}
	.login .ipt-b{background-position:0 -180px;}
	.login .ipt-b-hover{background-position:0 -216px;}

	.login .ipt-b50{width:60px;height:31px;color:#373737;}
	.login .ipt-b50{background-position:0 -180px;}
	.login .ipt-b50-hover{background-position:0 -216px;}
	
	.login .btn{background-position:-108px -180px;}
	.login .btn:hover{background-position:-108px -216px;}
	.login select{margin-top:4px;height:20px;width:209px;}
	.login .autologin{margin-right:15px;}
	.login #autoLoginDiv{ position:absolute; width:180px; background-color:#ffefa4; border:1px #FF9900 solid; left: 592px; top:287px; text-align:left; padding:5px; line-height:150%; color:#dc6800; display:none;}
	.login .foget{font-size:14px;margin-left:10px;}
	.login .err{margin-left:5px;color:#f00;}
	.login .reg0{margin-top:10px;padding-top:10px;border-top:1px solid #e0e0e0;width:370px;margin-left:1px;font-size:14px;}
	.login .reg{margin-top:25px;padding-top:25px;border-top:1px solid #e0e0e0;width:370px;margin-left:1px;font-size:14px;}
	.login .reg span,
	.login .reg .btn{float:right;line-height:31px;text-decoration:none;}
	.login .reg span{margin-right:10px;color:#5d5d5d;}
	
	.login .down{margin-top:10px;padding-top:10px;border-top:1px solid #e0e0e0;width:370px;margin-left:1px;font-size:14px;}
	.login .down span,
	.login .down .btn{float:right;line-height:31px;text-decoration:none;}
	.login .down span{margin-right:10px;color:#5d5d5d;}

